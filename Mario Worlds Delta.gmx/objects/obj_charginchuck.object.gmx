            //Set the horizontal speed
            hspeed = random_range(-0.5, 0.5);
            
            //Disable alarms
            alarm[0] = -1;
            alarm[1] = -1;
            
            //Apply gravity to the coin
            ready = 1;
                                    
            //Jump a bit
            y--;
            if (other.swimming == 0)
                vspeed = random_range(-2, -4);
            else
                vspeed = random_range(-1.5, -3);  
        }
    }
    
    //Perform death event
    event_user(0);
}

//Destroy fireball
with (other) event_user(0);

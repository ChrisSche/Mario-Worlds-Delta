        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Track Grinder

//Default value
down = false;
state_reset = 0;

//Animate
image_speed = 0.8;

//State
state = "IN_LINE";

//Speed
spd = 1.5;

//Origin
xorig = sprite_width/2-1;
yorig = sprite_height/2-1;

//Steps to take per steps
step = 0;

//Whether the engine jumped from a track
ready = 0;

//Direction
direct = 270;

//Play 'Engine' sound
alarm[2] = 7;

//Set up direction based on modifier
if (place_meeting(x, y, obj_up))
    direct = 90;
else if (place_meeting(x, y, obj_left))
    direct = 180;
else if (place_meeting(x, y, obj_right))
    direct = 0;
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="2" enumb="2">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Play 'Engine' sound

//Repeat the process
alarm[2] = 4;

//Play 'Engine' sound
if (state == "IN_LINE")
&amp;&amp; (!outside_view(16))
&amp;&amp; (!audio_is_playing(snd_engine))
    audio_stop_play_sound(snd_engine, 0, false);
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="2" enumb="0">
      <action>
        <libid>1</libid>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Handle block movement if not static

if (ready == 1)
&amp;&amp; (backnforth == true) {

    //If the block is moving to the right.
    if (dir == 1) {
        
        //Cap horizontal speed.
        if (hspeed &gt; 1)
        hspeed = 1;
            
        //Change direction.
        if (x &gt; xstart+32)
            dir = -1;
    }
    
    //Otherwise, if the block is moving to the left.
    else if (dir == -1) {
    
        //Cap horizontal speed.
        if (hspeed &lt; -1)
        hspeed = -1;
            
        //Change direction.
        if (x &lt; xstart-32)    
            dir = 1;
    }
    
    //Increment / Decrement speed.
    hspeed += 0.0125*sign(dir);
}

//Wave
if (y &gt; ystart)
    vspeed -= 0.015;
else if (y &lt; ystart)
    vspeed += 0.015;
</string>

            <string>///Eerie logic

//Activate when close to view
if (point_in_rectangle(x,y,view_xview[view_current]-16,view_yview[view_current]-16,view_xview[view_current]+view_wview[view_current]+16,view_yview[view_current]+view_hview[view_current]+16)) &amp;&amp; (activated = false) {
    

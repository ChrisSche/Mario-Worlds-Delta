    if (instance_exists(obj_playerparent)) {
    
        if (x &gt; obj_playerparent.x)
            hspeed = -1;
        else 
            hspeed = 1;
    } 
    else hspeed = 1;
}

//Handle image scale
if (hspeed &lt; 0)
    xscale = -1;
else 
    xscale = 1;

//Make destroyable when in view
if (activated = 1) 
&amp;&amp; (point_in_rectangle(x,y,view_xview[view_current]+16,view_yview[view_current]+16,view_xview[view_current]+view_wview[view_current]-16,view_yview[view_current]+view_hview[view_current]-16))
    activated = 2;

//Destroy when out of view 
if (object_index != obj_slimebubble)
    if (outside_view(16)) &amp;&amp; (activated = 2)
        instance_destroy();
</string>

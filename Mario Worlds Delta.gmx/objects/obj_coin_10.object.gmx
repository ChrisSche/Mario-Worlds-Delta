          <argument>
            <kind>1</kind>
            <string>///Collect the coin

//If not collected
if (image_speed != 1) {

    //Play 'Big Coin' sound
    audio_stop_play_sound(snd_coin_big, 0, false);
    
    //Increment coins
    global.coins += 10;
    
    //Set yprev and make visible
    yprev = y;
    visible = 1;
    alarm[0] = -1;
    alarm[1] = -1;
    
    //Set vertical speed
    vspeed = -4;
    gravity = 0.2;
    
    //Animate faster
    image_speed = 1;
    
    //Create sparkles
    var a;
    a = 0;
    repeat (6) {
    
        with (instance_create(x+8, y+8, obj_sparkle)) motion_set(a, 1);
        with (instance_create(x+8, y+8, obj_sparkle)) motion_set(a + 30, 2);
        a += 60;
    }
    
    //Get 100 points
    with (instance_create(x, y, obj_score)) 
        event_user(1);
}
</string>

    //Draw lakitu reel
    draw_sprite_ext(spr_lakitu_reel, type, round(x)-baitx, round(y)-baity, 1, 1, 0, c_white, 1);
    
    //Draw bait to avoid lag
    if instance_exists(bait)
        draw_sprite(bait.sprite_index,bait.image_index,x-baitx,y+baitlength);        
}

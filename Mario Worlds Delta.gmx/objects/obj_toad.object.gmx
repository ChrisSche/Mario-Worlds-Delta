        <arguments>
          <argument>
            <kind>1</kind>
            <string>///It's Toad, but outside the castle!

//Inherit event from obj_playerparent
event_inherited();

//Jump Strength
jumpstrength = 3.23375;
jumpstrength_spin = 3.0584375;

//Acceleration
acc = 0.1;

//Acceleration (when turning)
acc_turn = 0.2;

//Deceleration
dec = 0.05;

//Gravity
grav = 0.3625;

//Gravity (when jumping)
grav_alt = 0.0625;

//Gravity (when holding the jump button while falling)
grav_hold = 0.25;

//In water acceleration
acc_swim = 0.025;

//In water acceleration (when turning)
acc_turn_swim = 0.05;

//In water deceleration
dec_swim = 0.025;

//Gravity (Underwater)
grav_swim = 0.03;

//Minimum horizontal speed
hspeed_walk = 1.5;

//Maximum horizontal speed
hspeed_run = 3;

//Maximum horizontal speed (Full Meter)
hspeed_run_full = 3.5;

//Max falling speed
vspeedmax = 4;
</string>

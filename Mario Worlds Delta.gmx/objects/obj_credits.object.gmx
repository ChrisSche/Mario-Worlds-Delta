        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Update text

//Set background position
background_x[0] += 0.5;
background_x[1] += 1;

//Draw first the background
if (ready == 1) {

    alpha += 0.025;
    if (alpha &gt; 1) {
    
        alpha = 1;
        ready = 2;
        alarm[2] = 240;
    }
}
else if (ready == 3) {

    alpha -= 0.025;
    if (alpha &lt; 0) {
    
        alpha = 0;
        ready = 4;
    }
}

//Update yy
if (ready == 4) {

    yy += 0.4;
    if (yy &gt; 2024) {
    
        alpha += 0.025;
        if (alpha &gt; 1) {
        
            alpha = 1;
            ready = 5;
            alarm[3] = 60;
        }
    }
}
</string>
          </argument>
        </arguments>
      </action>

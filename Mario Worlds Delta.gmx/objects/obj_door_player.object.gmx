        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Replace door colour and set default frame

//Destroy door object
with (obj_player_door) alarm[1] = 2;

//Set default frame
image_index = 0;

//Set new player
switch (global.player) {

    //Mario -&gt; Luigi
    case (0): global.player = 1; break;
    
    //Luigi -&gt; Toad
    case (1): global.player = 2; break;
    
    //Toad -&gt; Peach
    case (2): global.player = 3; break;
    
    //Peach -&gt; Mario
    case (3): global.player = 0; break;
}

//Create a new player with the new parameters
with (player_create(x+8, y)) {

    invulnerable = 1;
    alarm[0] = 90;
    alarm[1] = 1;
}
</string>

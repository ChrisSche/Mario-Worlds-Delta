            <string>///Coffee Monty logic

//Previous hspeed
prevhspeed = hspeed;

//Inherit event
event_inherited();

//If the direction changed, speed up
if (hspeed != prevhspeed) {

    hspeed = sign(hspeed)*1.5;
    if (gravity == 0) {
    
        vspeed = -1.5;
        y--;
    }
}
    
//Keep charging at the player
if (charge == 1) {

    //If there's no gravity
    if (gravity == 0) {
        
        //Follow the player
        if (!instance_exists(obj_playerparent))
        || (obj_playerparent.x &lt; x) {
        
            hspeed -= 0.0324;
            if (hspeed &lt; -1.5)
                hspeed = -1.5;
        }
        else {
        
            hspeed += 0.0324;
            if (hspeed &gt; 1.5)
                hspeed = 1.5;
        }
    }
}

//Face towards the player
if (!instance_exists(obj_playerparent))
|| (obj_playerparent.x &lt; x)
    xscale = -1;
else
    xscale = 1;

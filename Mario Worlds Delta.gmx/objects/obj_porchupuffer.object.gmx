        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Porchu puffer logic

//If not chargin at the player
if (charge == 0) {

    //Activate when close to view
    if (point_in_rectangle(x, y, view_xview[view_current]-32, view_yview[view_current]-16, view_xview[view_current]+view_wview[view_current]+32, view_yview[view_current]+view_hview[view_current]+16))
        charge = 1;
}

else if (charge == 1) {
    
    //Chase the player
    if (!instance_exists(obj_playerparent)) //If the player does not exists or it's at the left
    || (obj_playerparent.x &lt; x) {
    
        hspeed -= 0.025;
        xscale = -1;
        if (hspeed &lt; -1.5)
            hspeed = -1.5;    
    }
    
    //Otherwise, go to the right
    else if (obj_playerparent.x &gt; x) {
    
        hspeed += 0.025;
        xscale = 1;
        if (hspeed &gt; 1.5)
            hspeed = 1.5;
    }
    
    //Make sure it does not exit the view boundaries
    if (x &lt; view_xview[view_current]-32)
        x = view_xview[view_current]-32;
    else if (x &gt; view_xview[view_current] + view_wview[view_current]+32)
        x = view_xview[view_current] + view_wview[view_current]+32;
}

//Set y position while on water
if (charge)
&amp;&amp; (collision_rectangle(bbox_left, bbox_top, bbox_right, y, obj_swim, 0, 0))
    y -= 0.25;
</string>

//40 Bonus stars
else if (y &lt; ystart-112)
    global.bonusadd = 40;

//1 - 30 Bonus stars
else if (y &gt;= ystart-112)
    global.bonusadd = round((ystart - y) / 112 * 29 + 1);

//Turn the player onto the goal object
with (other) {

    with (instance_create(x, y, obj_player_clear)) {
    
        vspeed = other.vspeed;
    }
    instance_destroy();
}

//Reveal how many stars were obtained

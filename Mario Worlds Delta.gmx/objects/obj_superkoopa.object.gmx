if (feather == 1) {

    //Create a feather
    instance_create(x, y, obj_feather_sprout);
    
    //Turn into a regular blue beach koopa
    with (instance_create(x, y+2, obj_beachkoopa_blue)) {
    
        //Set the horizontal speed
        hspeed = 1*sign(other.xscale);
        
        //Ignore alarm
        alarm[10] = -1;

            <kind>1</kind>
            <string>///Allow block to be hit again and swap player

//Play 'Switch' sound
audio_play_sound(snd_switch, 0, false);

//Allow block hit
ready = 0;

//Change player
with (obj_playerparent) {

    //Change player variable
    global.player++;
    
    //If the player is greater than 3
    if (global.player &gt; 2) then global.player = 0;
    
    //Create a new player
    with (player_create(x, y)) {
    
        //Hereby current state
        state = other.state;
    
        //Hereby motion
        hspeed = other.hspeed;
        vspeed = other.vspeed;
        
        //Hereby facing direction
        xscale = other.xscale;
    
        //Create a poof of smoke
        with (instance_create(x, y, obj_smoke))
            sprite_index = spr_smoke_32;
    }
    
    //Destroy old player
    instance_destroy();
}
</string>

        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Generate bobombs when the player is inside the area

//If the player exists
if (instance_exists(obj_playerparent)) {

    //If the player is inside the spawn area, proceed
    if (obj_levelcontrol.gswitchon == false)
    &amp;&amp; (point_in_rectangle(obj_playerparent.x, obj_playerparent.y, xmin, 0, xmax, room_height)) {
    
        //Generate a bobomb
        instance_create(view_xview[0]+random_range(32, global.gw-32), view_yview[0]-32, obj_bobomb_parachute);
        
        //Repeat
        alarm[0] = 180;
    }
    
    //Otherwise, hold it
    else {
    
        //Hold event
        alarm[0] = 1;
        exit;
    }
}

//Otherwise, hold
else {

    //Hold event
    alarm[0] = 1;
    exit;
}
</string>

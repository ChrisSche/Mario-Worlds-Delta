        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Collect the coin

//If the coin is not collected
if (image_speed != 1) 
&amp;&amp; (other.vspeed != 0) { 

    //Play 'Coin' sound
    audio_stop_play_sound(snd_coin_big, 0, false);
    
    //Set the sprite
    sprite_index = spr_coin_ace_get;
    
    //Increment coins
    global.coins++;
    
    //Remember that the coin has been collected
    ds_map_add(global.dcoins, id, 1);
    
    //Get score
    with (instance_create(x, y, obj_score)) {
    
        //If 5 or more coins were collected, award an extra life
        if (ds_map_size(global.dcoins) &gt;= 5) {
        
            //Play '1-UP' sound
            audio_play_sound(snd_1up, 0, false);
            
            //Let the player know it
            event_user(9);
        }
        
        //Otherwise, give points based on how many coins you collected
        else        
            event_user(4+ds_map_size(global.dcoins)); 
    }
        
    //Set yprev and make visible
    yprev = y;
    visible = 1;
    alarm[0] = -1;
    alarm[1] = -1;

    //Set vertical speed
    vspeed = -4;
    gravity = 0.2;
    
    //Animate faster
    image_speed = 1;
    
    //Create sparkles
    var a;
    a = 0;
    repeat (6) {
    
        with (instance_create(x+8, y+8, obj_sparkle)) motion_set(a, 1);
        with (instance_create(x+8, y+8, obj_sparkle)) motion_set(a + 30, 2);
        a += 60;
    }
}
</string>

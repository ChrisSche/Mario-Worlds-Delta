          <argument>
            <kind>1</kind>
            <string>///Check for 1 block gaps and destroy blocks on the way

//If the object is not falling
if (fall == 0) {

    //If there's not a slope
    if (!collision_rectangle(x-2, bbox_bottom+4, x+2, bbox_bottom+8, obj_slopeparent, 1, 0)) {
    
        //If there's not a semisolid below
        if (!collision_rectangle(bbox_left, bbox_bottom, bbox_right, bbox_bottom+1, obj_semisolidparent, 1, 0)) {
        
            move_snap(4, 2);
            y += 3;
            fall = 1;
        }
    }

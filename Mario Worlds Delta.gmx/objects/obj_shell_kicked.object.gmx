//Check for blocks to the right    
var block_r = collision_rectangle(bbox_right, bbox_top, bbox_right+9+hspeed, bbox_bottom, obj_solidparent, 0, 0);
if (block_r)
&amp;&amp; (sprite_height &gt; 16)
    with (block_r) event_user(15);

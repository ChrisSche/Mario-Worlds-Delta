    else
        draw_text_colour(view_xview[0]+view_wview[0]-104, view_yview[0]+23, "000", c_white, c_white, c_white, c_white, 1);
}
else {

    draw_text_colour(view_xview[0]+view_wview[0]-104, view_yview[0]+23, string_add_zeros(obj_hud.time, 3), c_white, c_white, c_white, c_white, 1);

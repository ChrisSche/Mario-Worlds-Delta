        shard_create(x+8, y+8, shardsprite, 0, 6, 120);
        shard_create(x+8, y+8, shardsprite, 0, 6, 135);
        
        //Make the player bounce a little
        with (obj_playerparent) event_user(1);
            
        //Destroy
        instance_destroy();        
    }
}



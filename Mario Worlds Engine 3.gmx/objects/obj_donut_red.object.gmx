          <argument>
            <kind>1</kind>
            <string>///Red donut logic

//If not falling
if (vspeed == 0) {

    //If the player is above, shake
    if (collision_rectangle(bbox_left, bbox_top-1, bbox_right, bbox_top, obj_playerparent, 0, 0))
    &amp;&amp; (obj_playerparent.y &lt; y-11)
    &amp;&amp; (obj_playerparent.vspeed &gt;= 0)
    &amp;&amp; (obj_playerparent.state != statetype.jump)
        shake = 1;

    //Otherwise, wait to shake.
    else {
    
        shake = 0;
        alarm[0] = 7;
    }
}

//Otherwise
else {

    //Inherit event from parent
    event_inherited();

    //Do not shake
    shake = 2;
    
    //Cap vertical speed.
    if (vspeed &gt; 2)
        vspeed = 2;
}

//Destroy
if (shake == 2)
&amp;&amp; (bbox_top &gt; view_yview[0] + view_hview[0])
    event_user(0);
</string>
